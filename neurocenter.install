<?php

function neurocenter_field_schema($field) {
  switch ($field['type']) {
    case 'neurocenter':
      $columns = array(
        'serialized_network' => array(
          'type' => 'text',
          'size' => 'medium',
          'not null' => true));
  };

  return array(
    'columns' => $columns);
}
